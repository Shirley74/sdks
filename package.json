{
  "name": "@1inch/sdks",
  "version": "0.0.0",
  "private": true,
  "description": "1inch Protocol SDKs monorepo managed with NX",
  "scripts": {
    "nx": "nx",
    "graph": "nx graph",
    "affected": "nx affected",
    "affected:build": "nx affected -t build",
    "affected:test": "nx affected -t test",
    "affected:lint": "nx affected -t lint",
    "build": "nx run-many -t build",
    "test": "nx run-many -t test",
    "lint": "nx run-many -t lint",
    "format": "nx format:write",
    "format:check": "nx format:check",
    "clean": "nx run-many -t clean && rimraf node_modules dist .nx",
    "reset": "nx reset",
    
    "aqua:build": "nx build aqua",
    "aqua:test": "nx test aqua",
    "aqua:lint": "nx lint aqua",
    "aqua:serve": "nx serve aqua",
    
    "cross-chain:build": "nx build cross-chain",
    "cross-chain:test": "nx test cross-chain",
    "cross-chain:lint": "nx lint cross-chain",
    "cross-chain:serve": "nx serve cross-chain",
    
    "fusion:build": "nx build fusion",
    "fusion:test": "nx test fusion",
    "fusion:lint": "nx lint fusion",
    "fusion:serve": "nx serve fusion",
    
    "limit-order:build": "nx build limit-order",
    "limit-order:test": "nx test limit-order",
    "limit-order:lint": "nx lint limit-order",
    "limit-order:serve": "nx serve limit-order"
  },
  "keywords": ["1inch", "sdk", "monorepo", "nx", "typescript", "defi", "protocol"],
  "author": "@1inch",
  "license": "MIT",
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@9.0.0",
  "volta": {
    "node": "22.13.1"
  },
  "devDependencies": {
    "@1inch/eslint-config": "^3.0.6",
    "@1inch/tsconfig": "^1.0.8",
    "@nx/workspace": "^21.6.5",
    "@nx/js": "^21.6.5",
    "@nx/node": "^21.6.5",
    "@nx/web": "^21.6.5",
    "@nx/eslint": "^21.6.5",
    "@nx/jest": "^21.6.5",
    "@nx/devkit": "^21.6.5",
    "@swc/core": "^1.3.107",
    "@swc/cli": "^0.4.0",
    "@swc-node/register": "^1.9.2",
    "@types/node": "^22.13.0",
    "@typescript-eslint/eslint-plugin": "^8.32.0",
    "@typescript-eslint/parser": "^8.32.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.1.5",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typescript": "^5.7.3",
    "nx": "^21.6.5"
